- provide(:title, 'Edit Category')

.row
  .small-12.columns
    %small.float-right
      Last Modified
      = @category.updated_at.strftime('%m-%d-%Y')
      by
      = @category.user.username
.row
  .small-12.columns
    %h1
      Editing category

    = render 'form'

    = link_to 'Categories', categories_path
    \|
    = link_to 'Show', @category
